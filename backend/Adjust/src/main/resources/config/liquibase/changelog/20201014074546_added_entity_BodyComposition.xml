            <column name="waist_hip_ratio" type="double">
                <constraints nullable="true" />
            </column>
            <column name="ideal_weight_min" type="double">
                <constraints nullable="true" />
            </column>
            <column name="ideal_weight_max" type="double">
                <constraints nullable="true" />
            </column>
            <column name="best_weight" type="double">
                <constraints nullable="true" />
            </column>
            <column name="bmi" type="double">
                <constraints nullable="true" />
            </column>
            <column name="bmi_category" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="bmr" type="double">
                <constraints nullable="true" />
            </column>
            <column name="lbm" type="double">
                <constraints nullable="true" />
            </column>
            <column name="pbf" type="double">
                <constraints nullable="true" />
            </column>
            <column name="body_fat_category" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="smm" type="double">
                <constraints nullable="true" />
            </column>
            <column name="body_type_number" type="double">
