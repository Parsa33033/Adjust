            <column name="visceral_fat_level" type="double">
                <constraints nullable="true" />
            </column>
            <column name="workout_location" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="goal" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="activity_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="workout_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <changeSet id="20201014074546-1-relations" author="jhipster">

    </changeSet>
