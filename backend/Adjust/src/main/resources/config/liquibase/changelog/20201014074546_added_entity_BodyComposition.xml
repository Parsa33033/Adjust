        <property name="autoIncrement" value="true"/>

    <!--
        Added the entity BodyComposition.
    -->
    <changeSet id="20201014074546-1" author="jhipster">
        <createTable tableName="body_composition">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
